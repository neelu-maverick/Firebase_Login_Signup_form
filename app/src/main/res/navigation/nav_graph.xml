<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/LoginFragment">

    <fragment
        android:id="@+id/LoginFragment"
        android:name="com.example.firebase_login_signup_form.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">

        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_LoginFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.firebase_login_signup_form.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">

        <action
            android:id="@+id/action_registerFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.firebase_login_signup_form.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard">

        <action
            android:id="@+id/action_dashboardFragment_to_LoginFragment"
            app:destination="@id/LoginFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_categoriesFragment"
            app:destination="@id/categoriesViewFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_categoriesViewFragment"
            app:destination="@id/categoriesViewFragment" />

    </fragment>
    <fragment
        android:id="@+id/categoriesViewFragment"
        android:name="com.example.firebase_login_signup_form.CategoriesViewFragment"
        android:label="fragment_categories_view"
        tools:layout="@layout/fragment_categories_view" >
        <action
            android:id="@+id/action_categoriesViewFragment_to_dogsFragment"
            app:destination="@id/dogsFragment" />
        <action
            android:id="@+id/action_categoriesViewFragment_to_birdsFragment"
            app:destination="@id/birdsFragment" />
        <action
            android:id="@+id/action_categoriesViewFragment_to_catsFragment"
            app:destination="@id/catsFragment" />
        <action
            android:id="@+id/action_categoriesViewFragment_to_horsesFragment"
            app:destination="@id/horsesFragment" />
        <action
            android:id="@+id/action_categoriesViewFragment_to_rabbitsFragment"
            app:destination="@id/rabbitsFragment" />
        <action
            android:id="@+id/action_categoriesViewFragment_to_turtleFragment"
            app:destination="@id/turtleFragment" />
        <action
            android:id="@+id/action_categoriesViewFragment_to_fishFragment"
            app:destination="@id/fishFragment" />
    </fragment>
    <fragment
        android:id="@+id/dogsFragment"
        android:name="com.example.firebase_login_signup_form.petsfolder.dogspackage.DogsFragment"
        android:label="fragment_dogs"
        tools:layout="@layout/fragment_dogs" >
        <action
            android:id="@+id/action_dogsFragment_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/birdsFragment"
        android:name="com.example.firebase_login_signup_form.petsfolder.birdspackage.BirdsFragment"
        android:label="fragment_birds"
        tools:layout="@layout/fragment_birds" >
        <action
            android:id="@+id/action_birdsFragment_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/catsFragment"
        android:name="com.example.firebase_login_signup_form.petsfolder.catspackage.CatsFragment"
        android:label="fragment_cats"
        tools:layout="@layout/fragment_cats" >
        <action
            android:id="@+id/action_catsFragment_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/fishFragment"
        android:name="com.example.firebase_login_signup_form.petsfolder.fishpackage.FishFragment"
        android:label="fragment_fish"
        tools:layout="@layout/fragment_fish" >
        <action
            android:id="@+id/action_fishFragment_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/rabbitsFragment"
        android:name="com.example.firebase_login_signup_form.petsfolder.rabbitpackage.RabbitsFragment"
        android:label="fragment_rabbits"
        tools:layout="@layout/fragment_rabbits" >
        <action
            android:id="@+id/action_rabbitsFragment_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/horsesFragment"
        android:name="com.example.firebase_login_signup_form.petsfolder.horsepackage.HorsesFragment"
        android:label="fragment_horses"
        tools:layout="@layout/fragment_horses" >
        <action
            android:id="@+id/action_horsesFragment_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/turtleFragment"
        android:name="com.example.firebase_login_signup_form.petsfolder.turtlepackage.TurtleFragment"
        android:label="fragment_turtle"
        tools:layout="@layout/fragment_turtle" >
        <action
            android:id="@+id/action_turtleFragment_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/pictureFragment"
        android:name="com.example.firebase_login_signup_form.PictureFragment"
        android:label="fragment_picture"
        tools:layout="@layout/fragment_picture" />


</navigation>